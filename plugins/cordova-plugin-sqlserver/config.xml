<?xml version='1.0' encoding='utf-8'?>
<widget id="sqlserver.plugin" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>sqlserver plugin</name>
    <description>
		SQL Server remote access plugin.
	</description>
    <author email="thym-dev@eclipse.org" href="http://www.eclipse.org/thym">
		Eclipse.org - Thym
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="6.3.0-nightly.2017.8.15.14816c7c" />
    <engine name="ios" spec="4.5.0-nightly.2017.8.15.077b1dc3" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
